{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DataFactoryLava2022"
		},
		"Deptcsv_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Deptcsv'"
		},
		"Deptsql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Deptsql'"
		},
		"Outputsql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Outputsql'"
		},
		"inputblob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'inputblob'"
		},
		"trigger2_properties_pipeline1_parameters_SourcePath": {
			"type": "string",
			"defaultValue": "@triggerBody().filePath"
		},
		"trigger2_properties_pipeline1_parameters_SourceFile": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"trigger2_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/3d0b786f-0663-4a02-8925-507ed56e40d7/resourceGroups/Lavanya_RG/providers/Microsoft.Storage/storageAccounts/datafactorystoragelava"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DataFactoryLava2022_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Deptcsv_connectionString": {
						"value": "[parameters('Deptcsv_connectionString')]"
					},
					"Deptsql_connectionString": {
						"value": "[parameters('Deptsql_connectionString')]"
					},
					"Outputsql_connectionString": {
						"value": "[parameters('Outputsql_connectionString')]"
					},
					"inputblob_connectionString": {
						"value": "[parameters('inputblob_connectionString')]"
					},
					"trigger2_properties_pipeline1_parameters_SourcePath": {
						"value": "[parameters('trigger2_properties_pipeline1_parameters_SourcePath')]"
					},
					"trigger2_properties_pipeline1_parameters_SourceFile": {
						"value": "[parameters('trigger2_properties_pipeline1_parameters_SourceFile')]"
					},
					"trigger2_properties_typeProperties_scope": {
						"value": "[parameters('trigger2_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}